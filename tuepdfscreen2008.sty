  \newcommand{\slidetitle}[2][]{
%  \pause\pauselevel{=1}
  \if@afour
    \setlength{\titleheight}{25mm}
    \setlength{\titlewidth}{131mm}
  \else
    \if@official
      \setlength{\titleheight}{3cm}
      \setlength{\titlewidth}{17cm}
